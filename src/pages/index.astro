---
import Welcome from "../components/Welcome.astro";
import Biography from "../components/Biography.astro";
import Rezensionen from "../components/Rezensionen.astro";
import Leistungen from "../components/Leistungen.astro";
import Kontakt from "../components/Kontakt.astro";
import Layout from "../layouts/Layout.astro";
const links = [
  {
    text: "Biographie",
    to: "#biographie",
  },
  {
    text: "Leistungen",
    to: "#leistungen",
  },
  {
    text: "Rezensionen",
    to: "#rezensionen",
  },
  {
    text: "Kontakt",
    to: "#kontakt",
  },
];
const title =
  "Jakob Matuschek – Saxophonist Passau | Hochzeiten & Firmenevents";
const description =
  "Erleben Sie unvergessliche Momente mit Saxophonist Jakob Matuschek aus Passau. Live-Musik für Hochzeiten, Firmenevents und private Feiern. Jetzt buchen.";

// Welcome to Astro! Wondering what to do next? Check out the Astro documentation at https://docs.astro.build
// Don't want to use any of this? Delete everything in this file, the `assets`, `components`, and `layouts` directories, and start fresh.
---

<Layout links={links} title={title} description={description}>
  <Welcome />
  <Biography />
  <Leistungen />
  <Rezensionen />
  <Kontakt />
</Layout>
